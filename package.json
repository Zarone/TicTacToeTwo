{
  "name": "@tictactoe/workspace",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "repository": "git@github.com:posixpascal/TicTacToeTwo.git",
  "author": "oats-community",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn workspace @tictactoe/client start\" \"yarn workspace @tictactoe/server start\"",
