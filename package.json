    "format": "prettier --write ."
  },
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "concurrently": "^7.4.0",
    "pm2": "^5.2.2",
    "ts-node": "^10.9.1"
  }
